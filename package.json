{
  "name": "sendgrid-webhook-server-gtapolicemods",
  "version": "1.0.0",
  "description": "A webhook server for handling SendGrid Inbound Parse Webhooks, written in TypeScript and deployable to Vercel.",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "express": "^4.18.2",
    "mailparser": "^3.7.2",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mailparser": "^3.4.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT"
}
